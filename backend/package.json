{
  "name": "template-node",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "migrate": "yarn sequelize db:migrate",
    "migrate:prod": "set NODE_ENV=.env.production && sequelize db:migrate",
    "undo": "yarn sequelize db:migrate:undo:all",
    "seeds": "yarn sequelize db:seed:all",
    "dev": "nodemon src/server.js",
    "dev:prod": "env-cmd -f.env.production nodemon src/server.js",
    "build": "env-cmd -f.env.production sucrase ./src -d ./dist --transforms imports",
    "start": "env-cmd -f.env.production node dist/server.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "celebrate": "^12.1.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "date-fns": "^2.15.0",
    "dotenv": "^8.0.0",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-handlebars": "^5.2.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.18",
    "nodemailer-express-handlebars": "^4.0.0",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.9.4",
    "uuidv4": "^6.2.6",
    "youch": "^2.0.10",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-prettier": "^3.1.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "sequelize-cli": "^5.5.0",
    "sucrase": "^3.17.1"
  }
}
